{
    "version": 2,
    "builds": [{ "src": "api/**/*", "use": "@vercel/node" }],
    "routes": [{ "src": "/api/*", "methods": ["GET", "POST", "PUT", "DELETE"], "dest": "/api/*", "config": { "external": true, "headers": { "X-Timeout-Seconds": 60 } }, "headers": { "Cache-Control": "s-maxage=0" } }],
    "regions": ["bru"],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
    